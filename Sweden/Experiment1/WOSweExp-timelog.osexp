define loop stimuli
	set source_file ""
	set source table
	set skip 0
	set repeat 1
	set order random
	set offset no
	set item trial
	set description "Repeatedly runs another item"
	set cycles 18
	set continuous no
	set column_order "Condition;CategoryLeft;CategoryRight;CategoryCorrect;Exemplar"
	set break_if_on_first yes
	set break_if never
	setcycle 0 read1 "Läraren ställde sig på bordet igår."
	setcycle 0 invert1 "Igår...."
	setcycle 0 cond1 refl
	setcycle 0 nr1 101
	setcycle 0 arg1 NP
	setcycle 0 read2 "Bagaren kom inte för sent i måndags."
	setcycle 0 invert2 "I måndags…"
	setcycle 0 cond2 SS
	setcycle 0 nr2 120
	setcycle 0 arg2 NP
	setcycle 1 read1 "Han ställde sig på bordet igår."
	setcycle 1 invert1 "Igår...."
	setcycle 1 cond1 refl
	setcycle 1 nr1 102
	setcycle 1 arg1 PRO
	setcycle 1 read2 "Han kom inte för sent i måndags."
	setcycle 1 invert2 "I måndags…"
	setcycle 1 cond2 SS
	setcycle 1 nr2 121
	setcycle 1 arg2 PRO
	setcycle 2 read1 "Skådespelaren vände sig plötsligt mot publiken."
	setcycle 2 invert1 "Plötsligt…"
	setcycle 2 cond1 refl
	setcycle 2 nr1 103
