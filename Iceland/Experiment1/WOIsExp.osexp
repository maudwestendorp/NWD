define loop stimuli
	set source_file ""
	set source table
	set skip 0
	set repeat 1
	set order random
	set offset no
	set item trial
	set description "Repeatedly runs another item"
	set cycles 15
	set continuous no
	set column_order "Condition;CategoryLeft;CategoryRight;CategoryCorrect;Exemplar"
	set break_if_on_first yes
	set break_if never
	setcycle 0 read1 "Lögfræðingurinn bauð mér nýja vinnu í síðustu viku."
	setcycle 0 invert1 "Í síðustu viku..."
	setcycle 0 cond1 Los
	setcycle 0 nr1 1
	setcycle 0 arg1 NP
	setcycle 0 read2 "Hann fór ekki heim til foreldra sinna í fyrra."
	setcycle 0 invert2 "Í fyrra..."
	setcycle 0 cond2 SS
	setcycle 0 nr2 12
	setcycle 0 arg2 PRO
	setcycle 1 read1 "Bakarinn gaf mér nýtt brauð á hverjum degi."
	setcycle 1 invert1 "Á hverjum degi..."
	setcycle 1 cond1 Los
	setcycle 1 nr1 1
	setcycle 1 arg1 NP
	setcycle 1 read2 "Hann tók ekki strætisvagninn til vinnu í gær."
	setcycle 1 invert2 "Í gær..."
	setcycle 1 cond2 SS
	setcycle 1 nr2 13
	setcycle 1 arg2 PRO
	setcycle 2 read1 "Kennarinn gaf mér nýja bók í gær."
	setcycle 2 invert1 "Í gær..."
	setcycle 2 cond1 Los
	setcycle 2 nr1 2
	setcycle 2 arg1 NP
	setcycle 2 read2 "Kennarinn tók ekki strætisvagninn til vinnu í gær."
	setcycle 2 invert2 "Í gær..."
	setcycle 2 cond2 SS
	setcycle 2 nr2 13
	setcycle 2 arg2 NP
	setcycle 3 read1 "Leikarinn sendi mér löng bréf á hverjum degi."
	setcycle 3 invert1 "Á hverjum degi..."
	setcycle 3 cond1 Los
	setcycle 3 nr1 2
	setcycle 3 arg1 NP
	setcycle 3 read2 "Nemandinn fór ekki heim til foreldra sinna í fyrra."
	setcycle 3 invert2 "Í fyrra..."
	setcycle 3 cond2 SS
	setcycle 3 nr2 14
	setcycle 3 arg2 NP
	setcycle 4 read1 "Besti hlauparinn gafst upp í síðustu umferðinni í fyrra."
	setcycle 4 invert1 "Í fyrra..."
	setcycle 4 cond1 "Part-Sub"
	setcycle 4 nr1 3
	setcycle 4 arg1 NP
	setcycle 4 read2 "Bakarinn þvoði sér strax eftir vinnu í gær. "
	setcycle 4 invert2 "Í gær..."
	setcycle 4 cond2 "Refl-Los"
	setcycle 4 nr2 14
	setcycle 4 arg2 NP
	setcycle 5 read1 "Nýi nemandinn fór út á lífið í gær."
	setcycle 5 invert1 "Í gær..."
	setcycle 5 cond1 "Part-Sub"
	setcycle 5 nr1 3
	setcycle 5 arg1 NP
	setcycle 5 read2 "Hann rakaði sig fyrir vinnu á hverjum degi."
	setcycle 5 invert2 "Á hverjum degi..."
	setcycle 5 cond2 "Refl-Los"
	setcycle 5 nr2 15
	setcycle 5 arg2 PRO
	setcycle 6 read1 "Nýi nemandinn tók til í eldhúsinu í gær."
